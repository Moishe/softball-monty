import random

random.seed()

for i in range(0,5):
    winners = []
    for j in range(0,7):
        if j == 0 or j == 1:
            winner = 0
        elif j == 5 or j == 6:
            winner = 1
        else:
            winner = random.randrange(0,1)
        winners.append(winner)

    print winners

        
